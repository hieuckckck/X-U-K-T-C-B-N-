#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
char c[10001];
    scanf("%s",c);
    int k;
    scanf("%d",&k);
    for(int i=0;i<k;i++){
        printf("%c",c[i]);
    }
    printf("28tech");
    for(int i=k;i<strlen(c);i++){
        printf("%c",c[i]);
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
