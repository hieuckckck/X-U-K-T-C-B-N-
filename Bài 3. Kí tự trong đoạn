#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>
int main() {
char c[10001];
    scanf("%s",c);
    int tong=0;
    for(int i=0;i<strlen(c);i++){
        if(isdigit(c[i])){
            tong+=c[i]-'0';
        }
    }
    printf("%d",tong);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
