#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    string s;
    getline(cin,s);
   for(int i=0;i<s.length();i++){
       if(isdigit(s[i])){
           cout<<s[i];
       }
   }
    cout<<endl;
  for(int i=0;i<s.length();i++){
       if(isalpha(s[i])){
           cout<<s[i];
       }
   }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
